{
    "name": "fastpress/request",
    "description": "A powerful and flexible HTTP request handling library for PHP, optimized for use with the Fastpress framework. Provides an easy-to-use interface for accessing request data and handling common request operations.",
    "type": "library",
    "version": "0.1.2",
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "scripts": {
        "test": "phpunit",
        "fix-style": "phpcbf --standard=PSR12 src/",
        "lint": "phpcs --standard=PSR12 src/",
        "analyze": "phpstan analyse src/"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Fastpress\\Http\\": "src/Http/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Samayo",
            "email": "samayo@pm.me"
        }
    ],
    "keywords": ["http", "request", "php", "fastpress", "http request", "framework"],
    "homepage": "https://github.com/fastpress/request",
    "support": {
        "issues": "https://github.com/fastpress/request/issues",
        "source": "https://github.com/fastpress/request"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}
